package Chapter3;

public class Duck extends FarmAnimal {
    public Duck(String type,String name, String gender, double weight, int age) {
        super(type,name, gender, weight, age);
    }
    @Override
    public String makeSound(){
        return "Quack Quack";
    }
    @Override
    public String FeedingLoadingSchedule() {
        return getName()+": 8am-12pm-6pm";
    }
}
